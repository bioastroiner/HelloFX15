<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scenePane" prefHeight="599.0" prefWidth="474.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login_simple.SceneController">
   <children>
      <Label fx:id="nameLabel" layoutX="14.0" layoutY="6.0" prefHeight="17.0" prefWidth="445.0" text="Hello">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </Label>
      <Button fx:id="bomb_butt" layoutX="-2.0" layoutY="497.0" mnemonicParsing="false" onAction="#showCode" prefHeight="102.0" prefWidth="159.0" text="Click for nuclear code" />
      <Button fx:id="logoutButton" layoutX="313.0" layoutY="497.0" mnemonicParsing="false" onAction="#logout" prefHeight="102.0" prefWidth="159.0" text="Logout" />
      <ImageView fx:id="memeholder" fitHeight="451.0" fitWidth="452.0" layoutX="9.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@meme_1.png" />
         </image>
      </ImageView>
      <Button fx:id="memebutt" layoutX="154.0" layoutY="497.0" mnemonicParsing="false" onAction="#showMeme" prefHeight="102.0" prefWidth="159.0" text="Click for memes" />
   </children>
</AnchorPane>
